<Window x:Class="session7_projects.WrapPanel2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:session7_projects"
        mc:Ignorable="d"
        Title="WrapPanel2" Height="400" Width="400">
    <WrapPanel>
        <Ellipse Width="100" Height="100" Fill="Red"></Ellipse>
        <Button Background="Orange" Width="180" Height="50">Click me!</Button>
        <Separator Width="10"></Separator>
        <TextBox Height="50" Width="200"></TextBox>
        <Rectangle Width="100" Height="100" Fill="Green"></Rectangle>
    </WrapPanel>
</Window>
