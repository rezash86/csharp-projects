<Window x:Class="session11_projects.DatePickerExample"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:session11_projects"
        mc:Ignorable="d"
        Title="DatePickerExample" Height="300" Width="300">
    <StackPanel Margin="20">
        <Label>Name:</Label>
        <TextBox x:Name="txtname"></TextBox>
        <Label>Birthday:</Label>
        <DatePicker x:Name="dtPickerSample"></DatePicker>
        <Label>Gender:</Label>
        <ComboBox Name="cmbGender">
            <ComboBoxItem>Female</ComboBoxItem>
            <ComboBoxItem>Male</ComboBoxItem>
        </ComboBox>
        <Button Margin="20" Click="Button_Click">SignUp</Button>
    </StackPanel>
</Window>
