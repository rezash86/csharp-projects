<Window x:Class="session10_projects.FrameExample"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:session10_projects"
        mc:Ignorable="d"
        Title="FrameExample" Height="350" Width="520">
    <Grid Background="LightBlue">
        <StackPanel HorizontalAlignment="Left" Width="140">
            <Button x:Name="btnMsft" Content="Microsoft" Width="100" Height="40" Margin="10" Click="btnMsft_Click"></Button>
            <Button x:Name="btnIntel" Content="Intel" Width="100" Height="40" Margin="10" Click="btnIntel_Click"></Button>
        </StackPanel>
        <Frame x:Name="frame" Margin="125,0,5,5" Visibility="Hidden" Source="http://www.microsoft.com"></Frame>
        <Frame x:Name="frame1" Margin="125,0,5,5" Visibility="Hidden" Source="http://www.intel.com"></Frame>
    </Grid>
</Window>
